<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Label layoutX="158.0" layoutY="36.0" text="Main Foxxy Menu">
         <font>
            <Font name="SansSerif Regular" size="36.0" />
         </font></Label>
      <Button fx:id="mmEditButton" layoutX="127.0" layoutY="128.0" mnemonicParsing="false" onAction="#mmEditClick" style="-fx-background-color: #31d3d8;" text="Edit Profile" textFill="#fffbfb">
         <font>
            <Font name="System Bold" size="22.0" />
         </font></Button>
      <Button fx:id="ProfileButton" layoutX="359.0" layoutY="128.0" mnemonicParsing="false" onAction="#mmCreateClick" prefHeight="48.0" prefWidth="169.0" style="-fx-background-color: #31d3d8;" text="Your Profile" textFill="#fffafa">
         <font>
            <Font name="System Bold" size="22.0" />
         </font></Button>
      <Button layoutX="104.0" layoutY="213.0" mnemonicParsing="false" style="-fx-background-color: #314fd8;" text="Game Selection" textFill="#fcf9f9">
         <font>
            <Font name="System Bold" size="22.0" />
         </font></Button>
      <Button layoutX="131.0" layoutY="289.0" mnemonicParsing="false" prefHeight="86.0" prefWidth="327.0" style="-fx-background-color: #813ac9;" text="Start Matchmaking" textFill="#fcf8f8">
         <font>
            <Font name="System Bold" size="22.0" />
         </font></Button>
      <Button fx:id="mmFreindListButton" layoutX="344.0" layoutY="213.0" mnemonicParsing="false" onAction="#mmAddFriendClick" prefHeight="48.0" prefWidth="199.0" style="-fx-background-color: #65a82f;" text="Friend List" textFill="#fcf6f6">
         <font>
            <Font name="System Bold" size="22.0" />
         </font></Button>
   </children>
</AnchorPane>
