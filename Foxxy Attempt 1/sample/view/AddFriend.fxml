<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Button fx:id="searchAddFriendButton" layoutY="299.0" mnemonicParsing="false" onAction="#friendSearchClick" prefHeight="100.0" prefWidth="300.0" style="-fx-background-color: #92f442;" text="Search" textFill="#fcf8f8">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Button>
      <Button cancelButton="true" layoutX="300.0" layoutY="299.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="300.0" style="-fx-background-color: #f45f42;" text="Cancel" textFill="WHITE">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Button>
      <Label layoutX="143.0" layoutY="126.0" text="Enter a GamerID to Search and Add Friends">
         <font>
            <Font name="SansSerif Regular" size="16.0" />
         </font>
      </Label>
      <TextField layoutX="162.0" layoutY="200.0" prefHeight="42.0" prefWidth="278.0" promptText="(GamerID)">
         <font>
            <Font name="SansSerif Regular" size="14.0" />
         </font></TextField>
      <Pane prefHeight="100.0" prefWidth="600.0" style="-fx-background-color: #65a82f;">
         <children>
            <Label layoutX="216.0" layoutY="28.0" text="Add Friends" textFill="WHITE">
               <font>
                  <Font name="SansSerif Regular" size="36.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
